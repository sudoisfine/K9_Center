<%= simple_form_for(@dog) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :dob %>
    <%= f.input :checked_in %>
    <%= f.association :owner_id %>
    <%= f.association :vet_id %>
    <%= f.input :info %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
